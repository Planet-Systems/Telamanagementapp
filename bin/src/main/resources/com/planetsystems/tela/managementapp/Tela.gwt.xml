<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='Tela'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User' />

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard' />
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->

   <inherits name="com.google.gwt.resources.Resources" />
	<!-- <inherits name='com.gwtplatform.mvp.Mvp' /> -->
	<inherits name='com.gwtplatform.mvp.MvpWithEntryPoint' />
	<inherits name='com.gwtplatform.dispatch.Dispatch' />

	<inherits name="com.smartgwt.tools.SmartGwtTools" />
	<inherits name='com.google.gwt.user.theme.clean.Clean' />


	<inherits name="com.smartgwt.SmartGwtNoScript" />
	<inherits name="com.smartgwt.SmartGwtNoTheme" />
	<!-- <inherits name="com.smartclient.theme.tahoe.Tahoe" /> -->

	<inherits name="com.smartclient.theme.stratus.Stratus" />
	
	<inherits name="com.planetsystems.tela.Model"></inherits>
	
	<!-- exclude compilation for safari -->
	<set-property name="user.agent" value="safari" />
	
	


  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.planetsystems.tela.managementapp.client.Tela' />

  <!-- Specify the paths for translatable code                    -->
  <source path='client' />
  <source path='shared' />
  
  <set-configuration-property
		name="gin.ginjector.modules"
		value="com.planetsystems.tela.managementapp.client.gin.ClientModule" />

</module>
